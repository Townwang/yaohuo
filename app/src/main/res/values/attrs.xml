<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="diffuse_color" format="color" />
    <attr name="diffuse_coreColor" format="color" />
    <attr name="diffuse_coreImage" format="reference" />
    <attr name="diffuse_coreRadius" format="float" />
    <attr name="diffuse_width" format="integer" />
    <attr name="diffuse_maxWidth" format="integer" />
    <attr name="diffuse_speed" format="integer" />

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color" />
        <attr name="diffuse_coreColor" />
        <attr name="diffuse_coreImage" />
        <attr name="diffuse_coreRadius" />
        <attr name="diffuse_width" />
        <attr name="diffuse_maxWidth" />
        <attr name="diffuse_speed" />
    </declare-styleable>


    <declare-styleable name="YaoItemView">
        <attr name="key" format="string" />
        <attr name="keyColor" format="color" />
        <attr name="keySize" format="dimension" />
        <attr name="value" format="string" />
        <attr name="valueColor" format="color" />
        <attr name="valueSize" format="dimension" />
        <attr name="valueHint" format="string" />
        <attr name="valueMaxLength" format="integer" />
        <attr name="isValueClickable" format="boolean" />
        <attr name="showRightIndicator" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ParticleView">
        <attr name="pv_host_text" format="string" />
        <attr name="pv_host_text_size" format="dimension" />
        <attr name="pv_particle_text" format="string" />
        <attr name="pv_particle_text_size" format="dimension" />
        <attr name="pv_text_color" format="color" />
        <attr name="pv_background_color" format="color" />
        <attr name="pv_text_anim_time" format="integer" />
        <attr name="pv_spread_anim_time" format="integer" />
        <attr name="pv_host_text_anim_time" format="integer" />
    </declare-styleable>

    <declare-styleable name="ConsecutiveScrollerLayout_Layout">
        <attr name="layout_isConsecutive" format="boolean" />
        <attr name="layout_isNestedScroll" format="boolean" />
        <attr name="layout_isSticky" format="boolean" />
        <attr name="layout_isTriggerScroll" format="boolean" />
        <attr name="layout_isSink" format="boolean" />
        <attr name="layout_align" format="enum">
            <enum name="LEFT" value="1" />
            <enum name="RIGHT" value="2" />
            <enum name="CENTER" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ConsecutiveScrollerLayout">
        <attr name="isPermanent" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TaurusHeader">
        <attr name="thPrimaryColor" format="color" />
        <attr name="srlPrimaryColor" />
    </declare-styleable>

    <attr name="type">
        <enum name="none" value="0" />
        <enum name="circle" value="1" />
        <enum name="rounded_rect" value="2" />
    </attr>
    <attr name="borderColor" format="color" />
    <attr name="borderWidth" format="dimension" />
    <attr name="rectRoundRadius" format="dimension" />

    <declare-styleable name="ImageViewPlus">
        <attr name="type" />
        <attr name="borderColor" />
        <attr name="borderWidth" />
        <attr name="rectRoundRadius" />
    </declare-styleable>

    <!--  FloatingActionButton  start-->
    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorDisabled" format="color" />
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_icon" format="reference" />
        <attr name="fab_size" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="fab_title" format="string" />
        <attr name="fab_stroke_visible" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AddFloatingActionButton">
        <attr name="fab_plusIconColor" format="color" />
    </declare-styleable>
    <declare-styleable name="FloatingActionsMenu">
        <attr name="fab_addButtonColorPressed" format="color" />
        <attr name="fab_addButtonColorNormal" format="color" />
        <attr name="fab_addButtonSize" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="fab_addButtonPlusIconColor" format="color" />
        <attr name="fab_addButtonStrokeVisible" format="boolean" />
        <attr name="fab_labelStyle" format="reference" />
        <attr name="fab_labelsPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="fab_expandDirection" format="enum">
            <enum name="up" value="0" />
            <enum name="down" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
    </declare-styleable>
    <!-- FloatingActionButton end-->
</resources>